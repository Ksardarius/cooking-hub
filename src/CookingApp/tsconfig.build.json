{
    "compilerOptions": {
      "outDir": "./dist",
      "declaration": true,
      "declarationMap": true,
      "emitDeclarationOnly": true,
      "jsx": "react-jsx",
      "target": "es2016",
      "module": "esnext",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "moduleResolution": "Bundler"
    }
}